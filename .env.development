# Development Environment Configuration
# Copy this file to .env for development

# Application Configuration
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# Security Configuration (Development only - not secure!)
SECRET_KEY=dev-secret-key-not-for-production
JWT_SECRET_KEY=dev-jwt-secret-key-not-for-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60

# Database Configuration (Development)
MONGODB_PASSWORD=password
REDIS_PASSWORD=

# External API Configuration
# Add your development API keys here
OPENAI_API_KEY=your-openai-api-key
DEEPSEEK_API_KEY=your-deepseek-api-key
QIANFAN_API_KEY=your-qianfan-api-key
QIANFAN_SECRET_KEY=your-qianfan-secret-key

# Data Source Configuration
TUSHARE_TOKEN=your-tushare-token
FINNHUB_API_KEY=your-finnhub-api-key

# Application Settings
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]
MAX_CONCURRENT_ANALYSES=3
ANALYSIS_TIMEOUT_MINUTES=15

# Logging Configuration
LOG_FORMAT=text
LOG_FILE_MAX_SIZE=50MB
LOG_FILE_BACKUP_COUNT=3

# Cache Configuration
CACHE_TTL_SECONDS=1800
REDIS_MAX_CONNECTIONS=10