# 生产环境配置
NODE_ENV=production

# 禁用开发模式登录（生产环境必须禁用）
VITE_ENABLE_DEV_LOGIN=false

# API 配置
REACT_APP_API_BASE_URL=https://your-production-api.com

# Authing 生产配置
VITE_AUTHING_APP_ID=your-production-app-id
VITE_AUTHING_APP_HOST=https://your-production-authing.cn
VITE_AUTHING_REDIRECT_URI=https://your-production-domain.com/auth/callback